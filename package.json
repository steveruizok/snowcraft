{
  "name": "snowcraft",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite --host\" \"HOST=localhost PORT=1234 npx partykit dev\" --kill-others",
    "dev:win": "concurrently \"vite --host\" \"set HOST=localhost&& set PORT=1234 && npx partykit dev\" --kill-others",
    "build": "tsc && vite build",
    "deploy": "tsc && vite build && npx partykit deploy",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
